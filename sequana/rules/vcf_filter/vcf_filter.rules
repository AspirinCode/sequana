rule vcf_filter:
    input:
        vcf = cfg.PROJECT + "/freebayes/%s.vcf" % cfg.PROJECT
    output:
        vcf = cfg.PROJECT + "/vcf_filter/%s.vcf" % cfg.PROJECT
    run:
        from sequana import vcf_filter
        vcf_record = vcf_filter.VCF(input["vcf"])
        vcf_record.filter_vcf(config["vcf_filter"], output["vcf"])
